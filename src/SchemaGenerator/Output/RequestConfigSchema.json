{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "title": "ListOfBasicVoiceRequest",
  "type": "array",
  "items": {
    "$ref": "#/definitions/BasicVoiceRequest"
  },
  "definitions": {
    "BasicVoiceRequest": {
      "type": "object",
      "additionalProperties": false,
      "properties": {
        "Key": {
          "type": "string"
        },
        "Phrases": {
          "type": "array",
          "items": {
            "type": "string"
          }
        },
        "Response": {
          "$ref": "#/definitions/SchrodingersString"
        }
      }
    },
    "SchrodingersString": {
      "type": "array",
      "items": {
        "$ref": "#/definitions/Possibility"
      }
    },
    "Possibility": {
      "type": "object",
      "additionalProperties": false,
      "properties": {
        "Text": {
          "type": "string"
        },
        "Weight": {
          "type": "number",
          "format": "double",
          "default": 1.0
        }
      }
    }
  }
}