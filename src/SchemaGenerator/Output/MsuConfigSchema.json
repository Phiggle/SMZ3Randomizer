{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "title": "MsuConfig",
  "type": "object",
  "additionalProperties": false,
  "properties": {
    "TrackLocations": {
      "type": "object",
      "additionalProperties": {
        "$ref": "#/definitions/SchrodingersString"
      }
    },
    "CurrentSong": {
      "oneOf": [
        {
          "type": "null"
        },
        {
          "$ref": "#/definitions/SchrodingersString"
        }
      ]
    },
    "CurrentMsu": {
      "oneOf": [
        {
          "type": "null"
        },
        {
          "$ref": "#/definitions/SchrodingersString"
        }
      ]
    },
    "UnknownSong": {
      "oneOf": [
        {
          "type": "null"
        },
        {
          "$ref": "#/definitions/SchrodingersString"
        }
      ]
    },
    "SongResponses": {
      "type": "object",
      "additionalProperties": {
        "$ref": "#/definitions/SchrodingersString"
      }
    }
  },
  "definitions": {
    "SchrodingersString": {
      "type": "array",
      "items": {
        "$ref": "#/definitions/Possibility"
      }
    },
    "Possibility": {
      "type": "object",
      "additionalProperties": false,
      "properties": {
        "Text": {
          "type": "string"
        },
        "Weight": {
          "type": "number",
          "format": "double",
          "default": 1.0
        }
      }
    }
  }
}